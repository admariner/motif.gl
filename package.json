{
  "name": "motif.gl",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "Timothy Lin",
  "license": "MIT",
  "repository": "cylynx/motif.gl",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --css-modules false",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false",
    "prepare": "run-s build",
    "prettier": "prettier ./src --write",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@antv/graphin": "^1.5.0",
    "@reduxjs/toolkit": "^1.2.3",
    "@types/d3-array": "^2.0.0",
    "@types/shortid": "0.0.29",
    "d3-array": "^2.5.0",
    "date-fns": "^2.14.0",
    "echarts": "^4.6.0",
    "echarts-for-react": "^2.0.0",
    "fibers": "^5.0.0",
    "fscreen": "^1.0.2",
    "html2canvas": "^1.0.0-rc.5",
    "immer": "^7.0.9",
    "json-2-csv": "^3.7.6",
    "lodash": "^4.17.15",
    "polished": "^4.0.3",
    "query-string": "5.1.1",
    "react-fast-compare": "^2.0.4",
    "react-hook-form": "^6.8.6",
    "react-hooks-full-screen": "0.0.1",
    "react-icons": "^3.11.0",
    "react-movable": "^2.5.3",
    "react-redux": "^7.1.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.1",
    "sass": "^1.26.5",
    "shortid": "^2.2.15",
    "type-analyzer": "^0.2.5"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "baseui": "^9.90.0",
    "styletron-engine-atomic": "^1.4.5",
    "styletron-react": "^5.2.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/echarts": "^4.6.4",
    "@types/fscreen": "^1.0.1",
    "@types/html2canvas": "^0.0.35",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.9",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "baseui": "^9.103.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-baseui": "^9.103.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "redux-mock-store": "^1.5.4",
    "styletron-engine-atomic": "^1.4.5",
    "styletron-react": "^5.2.7",
    "typescript": "^3.9.7"
  },
  "files": [
    "dist"
  ]
}
