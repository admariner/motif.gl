{
  "name": "motif.gl",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "Timothy Lin",
  "license": "MIT",
  "repository": "cylynx/motif.gl",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/motif.gl.esm.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --target node",
    "build": "tsdx build --target node",
    "prepare": "tsdx build --target node",
    "lint": "tsdx lint",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage --watchAll=false",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md)": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@antv/graphin": "^1.5.0",
    "@reduxjs/toolkit": "^1.2.3",
    "d3-array": "^2.8.0",
    "d3-axis": "^2.0.0",
    "d3-brush": "^2.1.0",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "date-fns": "^2.14.0",
    "decimal.js": "^10.2.1",
    "fibers": "^5.0.0",
    "fscreen": "^1.0.2",
    "html2canvas": "^1.0.0-rc.5",
    "immer": "^7.0.9",
    "json-2-csv": "^3.7.6",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "polished": "^4.0.3",
    "query-string": "5.1.1",
    "react-fast-compare": "^2.0.4",
    "react-hook-form": "^6.8.6",
    "react-hooks-full-screen": "0.0.1",
    "react-icons": "^3.11.0",
    "react-movable": "^2.5.3",
    "react-redux": "^7.1.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.1",
    "sass": "^1.26.5",
    "shortid": "^2.2.15",
    "styled-components": "^5.2.1",
    "type-analyzer": "^0.2.5"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "baseui": "^9.90.0",
    "styletron-engine-atomic": "^1.4.5",
    "styletron-react": "^5.2.7"
  },
  "size-limit": [
    {
      "path": "dist/motif.gl.cjs.production.min.js",
      "limit": "1 MB"
    },
    {
      "path": "dist/motif.gl.esm.js",
      "limit": "1 MB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@size-limit/preset-small-lib": "4.4.0",
    "@svgr/rollup": "^5.5.0",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^2.0.0",
    "@types/d3-brush": "^2.1.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-scale": "^3.2.1",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-time": "^2.0.0",
    "@types/d3-time-format": "^3.0.0",
    "@types/fscreen": "^1.0.1",
    "@types/html2canvas": "^0.0.35",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.9",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.4",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.0",
    "baseui": "^9.103.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-baseui": "^9.103.0",
    "lint-staged": "^10.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.4.1",
    "redux-mock-store": "^1.5.4",
    "rollup-plugin-postcss": "^3.1.8",
    "size-limit": "^4.9.1",
    "styletron-engine-atomic": "^1.4.5",
    "styletron-react": "^5.2.7",
    "tsdx": "^0.14.1",
    "webpack": "4.42.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
