{
  "name": "@cylynx/motif",
  "version": "0.0.1-beta.1",
  "description": "An out of the box graph visualization toolkit to assist analysis and investigation of network data.",
  "author": "Timothy Lin",
  "license": "MIT",
  "repository": "cylynx/motif.gl",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/motif.es.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "vite build --config vite.dev.config.ts",
    "build-tsdx": "tsdx build --target node",
    "build": "vite build --config vite.prod.config.ts",
    "lint": "tsdx lint",
    "test": "tsdx test",
    "test:watch": "tsdx test --watchAll",
    "test:coverage": "tsdx --coverage --watchAll=false",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "cypress:open": "cypress open",
    "cypress:test": "cypress run",
    "clean": "rimraf -rf ./dist",
    "preview": "vite preview"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "cypress"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "node_modules/lodash-es/.+\\.(j|t)sx?$": "ts-jest"
    },
    "setupFiles": [
      "jsdom-worker"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es|@cylynx/graphin)"
    ]
  },
  "dependencies": {
    "@antv/g6": "4.1.13",
    "@cylynx/graphin": "2.0.15",
    "@reduxjs/toolkit": "^1.5.0",
    "d3-array": "^2.8.0",
    "d3-axis": "^2.0.0",
    "d3-brush": "^2.1.0",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "date-fns": "^2.14.0",
    "decimal.js": "^10.2.1",
    "fibers": "^5.0.0",
    "fscreen": "^1.0.2",
    "html2canvas": "^1.0.0-rc.5",
    "immer": "^8.0.0",
    "json-2-csv": "^3.7.6",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "polished": "^4.0.3",
    "postcss": "^8.2.10",
    "query-string": "5.1.1",
    "react-fast-compare": "^2.0.4",
    "react-hook-form": "^6.8.6",
    "react-hooks-full-screen": "0.0.1",
    "react-icons": "^3.11.0",
    "react-movable": "^2.5.3",
    "react-redux": "^7.1.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.1",
    "sass": "^1.26.5",
    "shortid": "^2.2.15",
    "styled-components": "^5.2.1",
    "type-analyzer": "^0.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.12.13",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@rollup/plugin-url": "^6.0.0",
    "@size-limit/preset-small-lib": "4.4.0",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^2.0.0",
    "@types/d3-brush": "^2.1.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-scale": "^3.2.1",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-time": "^2.0.0",
    "@types/d3-time-format": "^3.0.0",
    "@types/fscreen": "^1.0.1",
    "@types/html2canvas": "^0.0.35",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.19.16",
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-window": "^1.8.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.4",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.0",
    "@types/webpack": "^4.41.26",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "css-loader": "^5.2.4",
    "cypress": "^6.4.0",
    "cypress-file-upload": "^5.0.2",
    "cypress-react-selector": "^2.3.6",
    "jsdom-worker": "^0.2.1",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.1",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "size-limit": "4.4.0",
    "style-loader": "^1.3.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "typescript": "^4.2.4",
    "vite": "^2.2.1",
    "webpack": "4.42.0",
    "workerloader-jest-transformer": "^0.0.2"
  },
  "peerDependencies": {
    "baseui": "^9.112.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styletron-engine-atomic": "^1.4.7",
    "styletron-react": "^5.2.7"
  },
  "size-limit": [
    {
      "path": "dist/motif.gl.cjs.production.min.js",
      "limit": "1 MB"
    },
    {
      "path": "dist/motif.gl.esm.js",
      "limit": "1 MB"
    }
  ],
  "files": [
    "dist"
  ]
}
