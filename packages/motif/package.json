{
  "name": "@cylynx/motif",
  "version": "0.0.1-beta.1",
  "description": "An out of the box graph visualization toolkit to assist analysis and investigation of network data.",
  "author": "Timothy Lin",
  "license": "MIT",
  "repository": "cylynx/motif.gl",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/motif.esm.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run clean && tsdx watch --target node",
    "build": "tsdx build --target node",
    "lint": "tsdx lint",
    "test": "tsdx test",
    "test:watch": "tsdx test --watchAll",
    "test:coverage": "tsdx --coverage --watchAll=false",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "cypress:open": "cypress open",
    "cypress:test": "cypress run",
    "clean": "rimraf -rf ./dist"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "cypress"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "node_modules/lodash-es/.+\\.(j|t)sx?$": "ts-jest"
    },
    "setupFiles": [
      "jsdom-worker"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es|@cylynx/graphin)"
    ]
  },
  "dependencies": {
    "@antv/g6": "4.1.13",
    "@cylynx/graphin": "2.0.15",
    "@reduxjs/toolkit": "^1.5.0",
    "d3-array": "^2.8.0",
    "d3-axis": "^2.0.0",
    "d3-brush": "^2.1.0",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "date-fns": "^2.14.0",
    "decimal.js": "^10.2.1",
    "fibers": "^5.0.0",
    "fscreen": "^1.0.2",
    "html2canvas": "^1.0.0-rc.5",
    "immer": "^8.0.0",
    "json-2-csv": "^3.7.6",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "polished": "^4.0.3",
    "query-string": "5.1.1",
    "react-fast-compare": "^2.0.4",
    "react-hook-form": "^6.8.6",
    "react-hooks-full-screen": "0.0.1",
    "react-icons": "^3.11.0",
    "react-movable": "^2.5.3",
    "react-redux": "^7.1.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.1",
    "sass": "^1.26.5",
    "shortid": "^2.2.15",
    "styled-components": "^5.2.1",
    "type-analyzer": "^0.2.5"
  },
  "peerDependencies": {
    "baseui": "^9.90.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styletron-engine-atomic": "^1.4.5",
    "styletron-react": "^5.2.7"
  },
  "size-limit": [
    {
      "path": "dist/motif.gl.cjs.production.min.js",
      "limit": "1 MB"
    },
    {
      "path": "dist/motif.gl.esm.js",
      "limit": "1 MB"
    }
  ],
  "files": [
    "dist"
  ]
}
