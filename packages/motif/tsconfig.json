{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "lib": ["dom", "dom.iterable", "esnext"],
    "outDir": "dist",
    "noImplicitAny": false,
    "noEmit": false,
    "types": [],
    "sourceMap": false
  },
  "include": ["src"],
  "exclude": [
